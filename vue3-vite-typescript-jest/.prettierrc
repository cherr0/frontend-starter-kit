{
  "tabWidth": 4,
  "printWidth": 120,
  "trailingComma": "es5",
  "semi": true,
  "singleQuote": true,
  "endOfLine": "auto",
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "overrides": [
    {
      "files": "*.vue",
      "options": {
        "htmlWhitespaceSensitivity": "ignore"
      }
    }
  ]
}
